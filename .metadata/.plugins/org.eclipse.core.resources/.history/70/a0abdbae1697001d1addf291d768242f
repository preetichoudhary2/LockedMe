package basic;


import java.util.Scanner;


public class Welcome {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Welcome to LockedMe.com");
		System.out.println("Developer Name:-Preeti Choudhary");
		System.out.println("you need to enter feature and file from cosole to perform the fuctionality");
		System.out.println("Enter feature");
		String feature = sc.next();
		System.out.println("Enter File name");
		int file = sc.nextInt();
		System.out.println("You want to "+feature+"the"+file);
		System.out.println("please enter Y/N to confirm ");
		
	}
}
